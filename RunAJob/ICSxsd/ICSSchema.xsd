<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <!--
    ===========================================================================
      Resource:         salesforceVersion
      Method:           GET
      Response Body:    salesforceVersion
      URI:              /api/v2/server/salesforceVersion
    ===========================================================================
  -->
  <xs:complexType name="salesforceVersion">
    <xs:sequence>
      <xs:element name="versionNo" type="xs:int"/>
    </xs:sequence>
  </xs:complexType>


  <!--
    ===========================================================================
      Resource:         serverTime
      Method:           GET
      Response Body:    serverTime
      URI:              /api/v2/server/serverTime
    ===========================================================================
  -->
  <xs:complexType name="serverTime">
    <xs:sequence>
      <xs:element name="time" type="xs:dateTime"/>
    </xs:sequence>
  </xs:complexType>

  <!--
    ===========================================================================
      Resource:         error
      Method:           GET, POST, DELETE
      Response Body:    error
      URI:              *
    ===========================================================================
  -->
  <xs:complexType name="error">
    <xs:sequence>
      <xs:element name="code" type="xs:string"/>
      <xs:element name="description" type="xs:string"/>
      <xs:element name="statusCode" type="xs:int"/>
    </xs:sequence>
  </xs:complexType>

  <!--
    ===========================================================================
      Resource:         login
      Method:           POST
      Request Body:     login
                            username, password
      Response Body:    user
      URI:              /api/v2/user/login
    ===========================================================================
  -->
  <!--
    ===========================================================================
      Resource:         loginSf
      Method:           POST
      Request Body:     login
                            sfSessionId, sfServerUrl
      Response Body:    user
      URI:              /api/v2/user/loginSf
    ===========================================================================
  -->
  <xs:complexType name="login">
    <xs:sequence>
      <xs:element name="username" type="xs:string" nillable="true"/>
      <xs:element name="password" type="xs:string" nillable="true"/>
      <xs:element name="sfSessionId" type="xs:string" nillable="true"/>
      <xs:element name="sfServerUrl" type="xs:string" nillable="true"/>
    </xs:sequence>
  </xs:complexType>

  <!--
    ===========================================================================
      Resource:         register
      Method:           POST
      Request Body:     registration
                            org
                                offerCode, name, employees, address1, address2(optional), 
                                address3(optional), city, state(required for United States),
                                zipcode, country
                            user
                                name, password, firstName, lastName, 
                                title, phone, emails, timeZone(optional)
                            registrationCode (optional)
      Response Body:    user
      URI:              /api/v2/user/register
    ===========================================================================
  -->
  <xs:complexType name="registration">
    <xs:sequence>
      <xs:element name="org" type="org"/>
      <xs:element name="user" type="user"/>
      <xs:element name="registrationCode" type="xs:string" nillable="true"/>
    </xs:sequence>
  </xs:complexType>
  
  <!--
    ===========================================================================
      Resource:         registerSf
      Method:           POST
      Request Body:     registrationSf
                            offerCode, sfSessionId, sfServerUrl, registrationCode (optional)
      Response Body:    user
      URI:              /api/v2/user/registerSf
    ===========================================================================
  -->
  <xs:complexType name="registrationSf">
    <xs:sequence>
      <xs:element name="sfSessionId" type="xs:string"/>
      <xs:element name="sfServerUrl" type="xs:string"/>
      <xs:element name="offerCode" type="xs:string"/>
      <xs:element name="registrationCode" type="xs:string" nillable="true"/>
    </xs:sequence>
  </xs:complexType>
  
